<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WIPActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Work In Progerss"
        android:textAppearance="@style/TextAppearance.Rubik.H2.Heavy"
        android:layout_gravity="center"
        tools:ignore="HardcodedText" />

    <id.co.edtslib.uikit.poinku.chip.BucketChip
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:stampCount="24"/>

    <id.co.edtslib.uikit.poinku.chip.BucketChipGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

</FrameLayout>